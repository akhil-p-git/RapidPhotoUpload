{
  "version": 2,
  "buildCommand": "cd apps/web && pnpm install && npx vite build",
  "outputDirectory": "apps/web/dist",
  "devCommand": "cd apps/web && pnpm dev",
  "installCommand": "pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
