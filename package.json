{
  "name": "rapid-photo-upload",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise-grade AI-enhanced media management platform with monorepo architecture",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter @rapid-photo/web dev",
    "dev:backend": "cd apps/backend && ./gradlew bootRun",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "n8n:dev": "cd apps/n8n && docker-compose up"
  },
  "devDependencies": {
    "@turbo/gen": "^2.3.0",
    "prettier": "^3.3.3",
    "turbo": "^2.3.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4"
  }
}
